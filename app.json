{
  "expo": {
    "name": "Ashley Inventory App",
    "slug": "ashleyInventory",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ashley_icon.png",
    "scheme": "ashleyinventory",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/ashley_splash1.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ashley_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.ashleyInventory",
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ashley_icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "94e8adcc-0e39-44e3-b788-81e205833af2"
      }
    }
  }
}
